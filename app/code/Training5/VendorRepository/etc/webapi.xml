<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/vendor/get/:id" method="GET">
        <service class="Training5\VendorRepository\Api\VendorRepositoryInterface" method="load"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/vendor/getList/" method="GET">
        <service class="Training5\VendorRepository\Api\VendorRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
</routes>
